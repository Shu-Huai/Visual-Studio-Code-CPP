        "cpp": "g++ '$fileName' -o '$fileNameWithoutExt.exe' -Wall -g -O2 -m64 -static-libgcc -std=c++17 -fexec-charset=GBK && &'./$fileNameWithoutExt.exe'"
    },
    "code-runner.saveFileBeforeRun": true,
